generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        String   @id @default(cuid())
  name      String
  price     Float?
  source    String?
  sales     Int?
  createdAt DateTime @default(now())
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  isVip     Boolean  @default(false)
  createdAt DateTime @default(now())
}
